# zkEmail Configuration
# Please provide these values for zkEmail integration

# Private key with Base Sepolia ETH for deployment
DEPLOYER_PRIVATE_KEY=your_funded_private_key_here

# zkEmail Relayer Configuration
ZKEMAIL_RELAYER_API_KEY=your_zkemail_relayer_api_key
ZKEMAIL_RELAYER_URL=https://relayer.zkemail.xyz

# Email Server Configuration for zkEmail
ZKEMAIL_EMAIL_SERVER=smtp.gmail.com
ZKEMAIL_EMAIL_PORT=587
ZKEMAIL_EMAIL_USER=your_email@gmail.com
ZKEMAIL_EMAIL_PASSWORD=your_app_specific_password

# zkEmail Circuit Configuration
ZKEMAIL_CIRCUIT_ARTIFACTS_URL=https://zkemail-circuits.s3.amazonaws.com/

# Optional: Custom DKIM keys for email providers
GMAIL_DKIM_KEY_HASH=
OUTLOOK_DKIM_KEY_HASH=
YAHOO_DKIM_KEY_HASH=